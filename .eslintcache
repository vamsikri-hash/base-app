 margin-left: 100px;\r\n  width: 400px;\r\n  label {\r\n    font-weight: 600;\r\n    font-size: large;\r\n  }\r\n`\r\nconst RedSpan = styled.span`\r\n  color: red;\r\n`\r\nexport const AddUser = () => {\r\n  const userContext = useContext(UserContext)\r\n\r\n  const { AddData } = userContext\r\n\r\n  const addUser = (values) => {\r\n    console.log(values)\r\n    AddData(values)\r\n    \r\n  }\r\n  const validateFields = (values) => {\r\n    console.info(values)\r\n    const errors = {}\r\n    if (!values.name || !values.name.trim()) {\r\n      errors.name = 'Required'\r\n    } \r\n    if (!values.email || !values.email.trim()) {\r\n      errors.email = 'Required'\r\n    } else if(!(/\\S+@\\S+\\.\\S+/.test(values.email))){\r\n      errors.email=\"email shoud be in form text@text.com\"\r\n    }\r\n    if (!values.phno || !values.phno.trim()) {\r\n      errors.phno = 'Required'\r\n    } else if (values.phno.trim().length<10){\r\n      errors.phno=\"Minimum 10 numbers required\"\r\n    }\r\n\r\n    return errors\r\n  }\r\n  return (\r\n    <FormContainer>\r\n      <Form\r\n        onSubmit={addUser}\r\n        validate={validateFields}\r\n        render={({ handleSubmit, values ,reset,form}) => (\r\n          <form onSubmit={handleSubmit}>\r\n            <Field name='name'>\r\n              {({ input, meta }) => (\r\n                <FormGroup label='Name' labelFor='text-input'>\r\n                  <InputGroup\r\n                    {...input}\r\n                    id='text-input'\r\n                    placeholder='Enter your Name'\r\n                  />\r\n                  {meta.error && meta.touched && (\r\n                    <RedSpan>{meta.error}</RedSpan>\r\n                  )}\r\n                </FormGroup>\r\n              )}\r\n            </Field>\r\n            <Field name='email'>\r\n              {({ input, meta }) => (\r\n                <FormGroup label='Email' labelFor='email'>\r\n                  <InputGroup\r\n                    {...input}\r\n                    id='email'\r\n                    placeholder='Enter your email'\r\n                  />\r\n                  {meta.error && meta.touched && (\r\n                    <RedSpan>{meta.error}</RedSpan>\r\n                  )}\r\n                </FormGroup>\r\n              )}\r\n            </Field>\r\n            <Field name='phno'>\r\n              {({ input, meta }) => (\r\n                <FormGroup label='Mobile' labelFor='text-input'>\r\n                  <InputGroup\r\n                    {...input}\r\n                    id='text-input'\r\n                    placeholder='Enter your mobile no'\r\n                  />\r\n                  {meta.error && meta.touched && (\r\n                    <RedSpan>{meta.error}</RedSpan>\r\n                  )}\r\n                </FormGroup>\r\n              )}\r\n            </Field>\r\n             \r\n            <Button intent={Intent.SUCCESS} onClick={async event=>{\r\n            console.log(event);\r\n            const error = await handleSubmit(event)\r\n            console.log(error);\r\n            if(error){\r\n                return error;\r\n            } else {\r\n               console.log(\"in form\");\r\n            form.restart()\r\n            AppToaster.show({message:\"Hey! user added successfully\",intent:Intent.SUCCESS})\r\n            }\r\n            \r\n          }}>\r\n              Submit\r\n            </Button>\r\n          </form>\r\n        )}\r\n      />\r\n    </FormContainer>\r\n  )\r\n}\r\n","/mnt/f/react-learnings/base-app/src/components/Users/userContext.js",[],"/mnt/f/react-learnings/base-app/src/components/Users/Users.js",["68"],"import { useEffect, useContext } from 'react'\r\nimport { User } from './User.js'\r\nimport { UserContext } from './userContext'\r\nimport styled from 'styled-components'\r\n\r\nconst UsersContainer = styled.div`\r\n  margin-top: 50px;\r\n  margin-left: 100px; \r\n  margin-right: 100px;\r\n  display: grid;\r\n  grid-area: users;\r\n  width: calc(100vh-300px);\r\n  grid-gap: 30px;\r\n  grid-template-columns: repeat(3, 1fr);\r\n`\r\n\r\nexport const Users = () => {\r\n  //const [Userdata, setUserdata] = useState([])\r\n  const { state, getData } = useContext(UserContext)\r\n\r\n  useEffect(() => {\r\n    console.log(1)\r\n    console.log(getData())\r\n    getData()\r\n  }, [])\r\n\r\n  return (\r\n    <UsersContainer>\r\n      {state.Userdata.map((user) => {\r\n        return <User key={user.id} user={user} />\r\n      })}\r\n    </UsersContainer>\r\n  )\r\n}\r\n\r\n// what is clean up in useEffect hook any example\r\n","/mnt/f/react-learnings/base-app/src/components/Users/userReducer.js",[],"/mnt/f/react-learnings/base-app/src/components/Users/User.js",["69","70","71","72","73","74","75"],"import { useContext, useState } from 'react'\r\nimport { UserContext } from './userContext'\r\nimport { Card, Button, Elevation, Intent, Alert } from '@blueprintjs/core'\r\nimport styled from 'styled-components'\r\nimport { ConfirmDeleteUser } from './ConfirmDeleteUser'\r\n\r\nconst StyledCard = styled(Card)``\r\nexport const User = ({ user }) => {\r\n \r\n  return (\r\n    <div>\r\n      <Card interactive={true} elevation={Elevation.FOUR}>\r\n        <h2>{user.name}</h2>\r\n        <h3>{user.email}</h3>\r\n        <h3>{user.phno}</h3>\r\n        <ConfirmDeleteUser userId={user.id} />\r\n      </Card>\r\n    </div>\r\n  )\r\n}\r\n","/mnt/f/react-learnings/base-app/src/components/Users/userActions.js",[],["76","77"],"/mnt/f/react-learnings/base-app/src/components/Users/ConfirmDeleteUser.js",[],"/mnt/f/react-learnings/base-app/src/components/AppToaster.js",["78"],"import React from 'react'\nimport { Position, Toaster } from \"@blueprintjs/core\";\n\nexport const AppToaster = Toaster.create({\n    className: \"recipe-toaster\",\n    position: Position.TOP,\n})\n","/mnt/f/react-learnings/base-app/src/components/Home/Home.js",["79"],"import React from 'react'\nimport styled from \"styled-components\"\nimport { Card, Eleva